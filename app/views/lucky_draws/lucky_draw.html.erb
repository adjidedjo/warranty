<!-- START panel-->
<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="panel panel-dark panel-flat">
      <div class="panel-heading text-center">
        Claim Your Lucky Draw.
      </div>
      <div class="panel-body">
        <%= form_for(lucky_draws_lucky_draw_path, {:class => "form-horizontal"}) do |f| %>

        <fieldset>
          <div class="form-group has-feedback">
            <label class="col-sm-2 control-label">Nama:</label>
            <div class="col-sm-3">
              <%= f.text_field :nama, class: "form-control", required: "required" %>
            </div>
            <label class="col-sm-2 control-label">Alamat:</label>
            <div class="col-sm-4">
              <%= f.text_field :alamat, class: "form-control", required: "required" %>
            </div>
          </div>
        </fieldset>

        <fieldset>
          <div class="form-group has-feedback">
            <label class="col-sm-2 control-label">No Telepon:</label>
            <div class="col-sm-3">
              <%= f.text_field :notelpon, class: "form-control", required: "required" %>
            </div>
            <label class="col-sm-2 control-label">Email:</label>
            <div id="registration_zip_code" class="col-sm-4">
              <%= f.text_field :email, class: "form-control", required: "required" %>
            </div>
          </div>
        </fieldset>

        <fieldset>
          <div class="form-group has-feedback">
            <label class="col-sm-2 control-label">Harga Pembelian:</label>
            <div class="col-sm-3">
              <%= f.text_field :harga, class: "form-control", required: "required", data: {:"data-parsley-equalto" => "number"} %>
            </div>
          </div>
        </fieldset>

        <fieldset>
          <div class="form-group has-feedback">
            <label class="col-sm-2 control-label">Upload Nota Pembelian:</label>
            <div class="col-sm-4">
              <%= f.file_field :faktur, class: "form-control", required: "required" %>
            </div>
          </div>
        </fieldset>

        <button type="submit" class="btn btn-block btn-primary mt-lg">
          CLAIM
        </button>
        <% end %>
      </div>
    </div>
  </div>
</div>
<!-- END panel-->
