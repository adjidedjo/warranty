<div class="content-heading">
  List Data Konfirmasi Display
</div>
<!-- START widgets box-->
<!-- END widgets box-->
<div class="row">
  <!-- START dashboard main content-->
  <div class="col-lg-12">
    <!-- START content-->
    <div class="row">
      <div class="col-lg-12">
        <div class="panel panel-info">
          <div class="panel-heading">
           Konfirmasi Display
          </div>
          <div class="panel-body">
            <table id="index-marketshare-brands" class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Area</th>
                  <th>Salesman</th>
                  <th>Toko</th>
                </tr>
              </thead>
              <tbody>
                <% @konfirmasi_displays.each do |kd| %>
                <tr class="gradeX">
			      <td><%= kd.area %></td>
			      <td><%= kd.salesman %></td>
			      <td><%= kd.customer %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>